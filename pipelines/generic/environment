## general configuration for the dataset

# status for this dataset setup
export PRODUCTION=0  # change the value to 1 when the dataset is ready for automatic processing

# short name for addressing the dataset
export DATASET=${DATASET}

# specify either a URL to download the dataset for this pipeline
# example: "https://data.beeldengeluid.nl/files/amateurfilm/lod_amateurfilm_sdo_20250806_clean.nt"
# or https://ams3.digitaloceanspaces.com/nde-europeana/nafotos.tar.gz
# https://ennomeijers.thegood.cloud/s/LbYtdzWJ9MfReSy/download/nafotos1.zip (nafotos1: 2-10-62 t/m 2-24-01-13)
# https://ennomeijers.thegood.cloud/s/8yAWSzd4cnfGR3Z/download/nafotos2.zip (nafotos2)
# https://ennomeijers.thegood.cloud/s/Jy7gAJDpMPfoGY3/download/nafotos3.zip (nafotos3)
export SOURCE_URL=

# or put on or more data files in the data directory and specify the path and filename separated by a ","
# for example: "./data/file1.nt,./data/file2.nt"
# when the SOURCE_URL is set the SOURCE_FILES variable will be set automatically
export SOURCE_FILES=${SOURCE_FILES_DOWNLOADED}

### variables used to automatically create the dataset description

# language of the dataset description 
# export DATASET_DESCRIPTION_LANGUAGE="en"  # example
export DATASET_DESCRIPTION_LANGUAGE=

# title of the dataset
# export DATASET_DESCRIPTION_TITLE="Images of the Dutch Regional Archives of Nijmegen for Europeana"  # example
export DATASET_DESCRIPTION_TITLE=

# URI for the organization that created the dataset
# export DATASET_DESCRIPTION_CREATOR_URI="https://www.regionaalarchiefnijmegen.nl/"  # example
export DATASET_DESCRIPTION_CREATOR_URI=

# name of the organization that created the dataset
# export DATASET_DESCRIPTION_CREATOR_NAME="Regional Archives of Nijmegen" # example
export DATASET_DESCRIPTION_CREATOR_NAME=

# creation date of the dataset
# export DATASET_DESCRIPTION_CREATE_DATE="2025-11-13" # example
export DATASET_DESCRIPTION_CREATE_DATE=

# source of the dataset
# export DATASET_DESCRIPTION_SOURCE_DATASET="<https://studiezaal.nijmegen.nl/AtlantisPubliek/data/dataset/LOD+Beelddocumenten>" # example
# TODO: the current setup require this variable to be a string wrapped URI, this is error prone
export DATASET_DESCRIPTION_SOURCE_DATASET=

# base location of the distribution 
# export DATASET_DESCRIPTION_DISTRUTION_BASE="https://nde-europeana.ams3.digitaloceanspaces.com";
export DATASET_DESCRIPTION_DISTRUTION_BASE=

